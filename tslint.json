{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "max-line-length": {
            "options": [140]
        },
        "no-any": false,
        "no-console": true,
        "no-var-requires": true,
        "quotemark": [
            true,
            "single",
            "avoid-escape",
            "jsx-single"
        ],
        "prefer-const": true,
        "no-shadowed-variable": true,
        "no-eval": true,
        "no-empty": true,
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "interface-name" : [true, "never-prefix"]
    },
    "rulesDirectory": []
}